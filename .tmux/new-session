# 一番初めのpaneを選択
select-pane -t 0
# 左右にウィンドウを分割
split-window -h
# 上下にウィンドウを分割する
split-window -v

# 1番目(下側)のウィンドウを選択
select-pane -t 1
# 上下にウィンドウを分割する
split-window -v

# 新しいウィンドウ追加
new-window

